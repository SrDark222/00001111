	Callback = function(v)
		local texto = v and "TCC ðŸ‡ªðŸ‡¬" or ""
		local tag = { Color3.new(1, 0, 0), 0, texto }
		game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvent"):WaitForChild("Shop"):WaitForChild("Tag"):FireServer(unpack(tag))
	end
})

